{
  "name": "fluoresce-editor",
  "version": "0.0.1",
  "description": "a colorful rich-text editor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "echo \"No tests yet\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylerwu2222/fluoresce-editor.git"
  },
  "keywords": [
    "rich",
    "text",
    "editor",
    "react"
  ],
  "author": "Tyler Wu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tylerwu2222/fluoresce-editor/issues"
  },
  "homepage": "https://github.com/tylerwu2222/fluoresce-editor#readme",
  "devDependencies": {
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.4",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
